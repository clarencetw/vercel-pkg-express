{
  "name": "vercel-pkg-express",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "bin": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "build": "npx pkg ."
  },
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*"
    ],
    "targets": [
      "linux",
      "macos",
      "win"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  }
}